<FindBugsFilter>
    <!-- Excluir todos los campos no usados en DTOs -->
    <Match>
        <Class name="com.example.myapp.dto.*"/>
        <Field name=".*"/>
        <Bug pattern="UUF_UNUSED_FIELD"/>
    </Match>

    <!-- Excluir anotaciones generadas automÃ¡ticamente -->
    <Match>
        <Class name="com.example.myapp.dto.*"/>
        <Field name=".*"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>
</FindBugsFilter>
